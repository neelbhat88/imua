<div class="container">
  <div class="row student-index">
    <div class="span6">
      <h1>Students</h1>
      <h2><%= link_to "Add a Student", new_admin_user_path %></h2>
      <hr>
      <ul>
        <% @users.each do |user| %>
          <li>
            <%= "#{user.first_name} #{user.last_name} (#{user.email})" %>
            <%= link_to 'Edit', edit_admin_user_path(user) %> |
            <%= link_to 'Profile', admin_user_path(user) %>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
